{
  "name": "mogami",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "yarn build:admin && yarn prisma generate && yarn build:api",
    "build:admin": "nx build admin --prod",
    "build:admin-sdk": "graphql-codegen --config libs/shared/util/admin-sdk/src/codegen.yml",
    "build:api": "nx build api --prod",
    "build:sdk": "yarn openapi-generator-cli generate -i ./api-swagger.json -g typescript-axios -o libs/sdk/src/generated && nx format:write --uncommitted",
    "dev:admin": "nx serve admin",
    "dev:admin-sdk": "yarn build:admin-sdk --watch",
    "dev:api": "nx serve api",
    "dev:demo": "nx serve demo",
    "dev:sdk": "nodemon --watch ./api-swagger.json -x 'yarn build:sdk'",
    "dev:services": "docker compose up",
    "docker:build": "docker build . -t ghcr.io/kin-labs/mogami",
    "docker:push": "docker push ghcr.io/kin-labs/mogami",
    "docker:run": "docker run -it -p 8000:3000 ghcr.io/kin-labs/mogami",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "prepare": "husky install",
    "start": "yarn prisma db push && node dist/apps/api/main.js",
    "test": "nx test && yarn test:admin-e2e && yarn test:demo-e2e",
    "test:admin-e2e:watch": "nx run admin-e2e:e2e --headed --watch",
    "test:admin-e2e": "nx run admin-e2e:e2e",
    "test:api-e2e:watch": "nx run api-e2e:e2e --verbose --watch",
    "test:api-e2e": "nx run api-e2e:e2e",
    "test:demo-e2e:watch": "nx run demo-e2e:e2e --headed --watch",
    "test:demo-e2e": "nx run demo-e2e:e2e"
  },
  "private": true,
