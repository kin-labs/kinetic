{
  "name": "mogami",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "yarn prisma generate && yarn build:api",
    "build:api": "nx build api --prod",
    "build:sdk": "yarn openapi-generator-cli generate -i ./api-swagger.json -g typescript-axios -o libs/sdk/src/generated && nx format:write --uncommitted",
    "dev:api": "nx serve api",
    "dev:demo": "nx serve demo",
    "dev:sdk": "nodemon --watch ./api-swagger.json -x 'yarn build:sdk'",
    "dev:services": "docker compose up",
    "docker:build": "docker build . -t ghcr.io/kin-labs/mogami",
    "docker:push": "docker push ghcr.io/kin-labs/mogami",
    "docker:run": "docker run -it -p 8000:3000 ghcr.io/kin-labs/mogami",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "prepare": "husky install",
    "start": "yarn prisma db push && node dist/apps/api/main.js",
    "test": "nx test"
  },
  "private": true,
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.2.0",
