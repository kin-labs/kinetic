    "bignumber": "^1.1.0",
    "bip39": "^3.0.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "core-js": "^3.6.5",
    "daisyui": "^2.10.0",
    "dexie": "^3.2.1",
    "dexie-react-hooks": "^1.1.1",
    "ed25519-hd-key": "^1.2.0",
    "joi": "^17.6.0",
