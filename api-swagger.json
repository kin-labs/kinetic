  "components": {
    "schemas": {
      "HealthCheckResponse": {
        "type": "object",
        "properties": {
          "isSolanaOk": {
            "type": "boolean"
          },
          "isMogamiOk": {
            "type": "boolean"
          },
          "time": {
            "type": "string"
          }
        },
        "required": ["isSolanaOk", "isMogamiOk", "time"]
      },
      "CreateAccountRequest": {
        "type": "object",
        "properties": {
          "tx": {
            "type": "string"
          }
        },
        "required": ["tx"]
      },
      "CreateAccountResponse": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string"
          },
          "account": {
            "type": "object"
          }
        },
        "required": ["status", "account"]
      },
      "BalanceResponse": {
        "type": "object",
        "properties": {
          "value": {
            "type": "string"
          }
        },
        "required": ["value"]
      },
      "HistoryResponse": {
        "type": "object",
        "properties": {
          "account": {
            "type": "object"
          },
          "history": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": ["account", "history"]
      },
      "AirdropRequest": {
        "type": "object",
        "properties": {
          "account": {
            "type": "string"
          },
          "amount": {
            "type": "string"
          }
        },
        "required": ["account"]
      },
      "AirdropResponse": {
        "type": "object",
        "properties": {
          "signature": {
            "type": "string"
          }
        },
        "required": ["signature"]
      },
      "AirdropStatsCounts": {
        "type": "object",
        "properties": {
          "averageValue": {
            "type": "number"
          },
          "total": {
            "type": "number"
          },
          "totalValue": {
            "type": "number"
          }
        },
        "required": ["averageValue", "total", "totalValue"]
      },
      "AirdropStats": {
        "type": "object",
        "properties": {
          "counts": {
            "$ref": "#/components/schemas/AirdropStatsCounts"
          },
          "dates": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        },
        "required": ["counts", "dates"]
      },
      "ApiConfigSummary": {
        "type": "object",
        "properties": {
          "port": {
            "type": "number"
          },
          "environment": {
            "type": "string"
          },
          "solanaRpcEndpoint": {
            "type": "string"
          }
        },
        "required": ["port", "environment", "solanaRpcEndpoint"]
      },
      "ServiceConfigResponse": {
        "type": "object",
        "properties": {
          "mint": {
            "type": "string"
          },
          "subsidizer": {
            "type": "string"
          },
          "tokenProgram": {
            "type": "string"
          }
        },
        "required": ["mint", "subsidizer", "tokenProgram"]
      },
      "MinimumKinVersionResponse": {
        "type": "object",
        "properties": {
          "version": {
            "type": "number"
          }
        },
        "required": ["version"]
      },
      "RecentBlockhashResponse": {
        "type": "object",
        "properties": {
          "blockhash": {
            "type": "string"
          }
        },
        "required": ["blockhash"]
      },
      "MinimumBalanceForRentExemptionResponse": {
        "type": "object",
        "properties": {
          "lamports": {
            "type": "number"
          }
        },
        "required": ["lamports"]
      },
      "SubmitPaymentRequest": {
        "type": "object",
        "properties": {
          "tx": {
            "type": "string"
          }
        },
        "required": ["tx"]
      },
      "SubmitTransactionResponse": {
        "type": "object",
        "properties": {}
      }
    }
